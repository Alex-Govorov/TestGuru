<h1><%= @test.title%></h1>
<strong>Level: <%= @test.level%></strong>

<h2>Listing Questions</h1>
<p><%= link_to "New question", new_test_question_path(@test) %></p>
<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @test.questions %>
  </tbody>
</table>

<p><%= link_to "Back", tests_path %></p>
